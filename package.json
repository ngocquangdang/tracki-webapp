{
  "name": "tracki-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon next dev",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --coverage",
    "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts --no-error-on-unmatched-pattern",
    "pretty": "prettier --write --config .prettierrc.json {src,pages,server}/**/*.{ts,tsx}",
    "format": "npm run lint -- --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "export": "cross-env NODE_ENV=production next export",
    "storybook": "start-storybook -s ./public/static -p 6006",
    "build-storybook": "build-storybook -s ./public/static  -c .storybook -o .storybookout"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@next/bundle-analyzer": "^9.4.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "autoprefixer": "^9.8.0",
    "axios": "^0.19.2",
    "connected-react-router": "^6.8.0",
    "cross-env": "^7.0.2",
    "cryptr": "^6.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "history": "^5.0.0",
    "http-proxy-middleware": "^1.0.4",
    "immer": "^7.0.4",
    "invariant": "^2.2.4",
    "lodash": "^4.17.15",
    "next": "9.4.4",
    "next-auth": "^2.2.0",
    "next-compose-plugins": "^2.2.0",
    "next-i18next": "^4.4.2",
    "next-redux-wrapper": "^6.0.2",
    "next-routes": "^1.4.2",
    "next-runtime-dotenv": "^1.2.0",
    "node-sass": "^4.14.1",
    "query-string": "^6.13.1",
    "react": "16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-is": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.1.1",
    "toastr": "^2.1.4",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@types/express": "^4.17.6",
    "@types/history": "^4.7.6",
    "@types/invariant": "^2.2.33",
    "@types/lodash": "^4.14.156",
    "@types/material-ui": "^0.21.7",
    "@types/next-redux-wrapper": "^3.0.0",
    "@types/react": "^16.9.36",
    "@types/react-icons": "^3.0.0",
    "@types/cryptr": "^4.0.1",
    "@types/toastr": "^2.1.38",
    "@types/react-redux": "^7.1.9",
    "@types/styled-components": "^5.1.0",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest-dom": "^3.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-app": "^1.1.2",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0"
