{
  "name": "tracki-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "dev": "nodemon next dev",
    "build": "next build && tsc --project tsconfig.server.json",
    "build:development": "env-cmd -f environments/.env.development next build",
    "build:preproduction": "env-cmd -f environments/.env.preproduction next build",
    "build:production": "env-cmd -f environments/.env.production next build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --coverage",
    "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts --no-error-on-unmatched-pattern",
    "pretty": "prettier --write --config .prettierrc.json {src,pages,server}/**/*.{ts,tsx}",
    "format": "npm run lint -- --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "export": "cross-env NODE_ENV=production next export",
    "storybook": "start-storybook -s ./public/static -p 6006",
    "build-storybook": "build-storybook -s ./public/static  -c .storybook -o .storybookout"
  },
